@model IEnumerable<Doge.Areas.Admin.Controllers.LogEntry>

@{
    ViewData["Title"] = "BrowseLog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Browse DogeLog</h1>



<table class="table">
    <thead>
        <tr>
            <th>
                Date and Time
            </th>
            <th>
                Level
            </th>
            <th>
                What happened
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.t)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mt)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SourceContext)
                </td>
            </tr>
        }
    </tbody>
</table>
